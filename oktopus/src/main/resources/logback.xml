<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-4level %logger{36} %L - %msg%n</pattern>
    </encoder>
  </appender>
  <logger name="com.amaizeing.oktopus" level="INFO"/>
  <root level="debug">
    <appender-ref ref="STDOUT"/>
  </root>
</configuration>
